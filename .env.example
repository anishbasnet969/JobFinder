GOOGLE_API_KEY=your_google_api_key_here
GEMINI_EMBEDDING_MODEL=gemini-embedding-001

CORS_ORIGINS="*"

POSTGRES_USER=app
POSTGRES_PASSWORD=app
POSTGRES_DB=jobfinderdb
POSTGRES_HOST=db
POSTGRES_PORT=5432
DATABASE_URL=postgresql+asyncpg://app:app@db:5432/jobfinderdb

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# Celery Configuration
CELERY_BROKER_URL=amqp://guest:guest@rabbitmq:5672//
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Performance Settings
CACHE_RESUME_TTL=86400
CACHE_RECOMMENDATIONS_TTL=3600
CACHE_EMBEDDINGS_TTL=604800
VECTOR_SEARCH_CANDIDATES=50

# Job ingestion
JOB_INGEST_ON_STARTUP=true
JOB_INGEST_CSV_PATH=datasets/Job/job_title_descriptions.csv
JOB_INGEST_BATCH_SIZE=50
JOB_INGEST_LIMIT=500

COHERE_API_KEY=your_cohere_api_key_here

# Reranking configuration (LLM-based reranking of vector search results)
# Enable/disable reranking and tune candidate counts
RERANKING_ENABLED=true
RERANKING_INPUT_CANDIDATES=50
RERANKING_OUTPUT_CANDIDATES=25